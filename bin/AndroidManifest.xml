<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hw3"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TriviaActivity"
            android:label="@string/title_activity_trivia" >
        </activity>
        <activity
            android:name=".QuestionTestActivity"
            android:label="@string/title_activity_question_test" >
        </activity>
        <activity
            android:name=".CreateQuestionActivity"
            android:label="@string/title_activity_create_question" >
        </activity>
        <activity
            android:name=".CreateQuestionAsyncTask"
            android:label="@string/title_activity_create_question_async_task" >
        </activity>
        <activity
            android:name=".ResultActivity"
            android:label="@string/title_activity_result" >
        </activity>
    </application>

</manifest>
